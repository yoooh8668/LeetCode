#define MIN(X, Y) (((X) < (Y)) ? (X) : (Y))
#define MAX(X, Y) (((X) < (Y)) ? (Y) : (X))
int maxArea(int* height, int heightSize){
    int result=0;
    int level=0;
    for (int i=0 ; i<heightSize ; i++)
    {
        for (int j=i+1 ; j<heightSize ; j++)
        {
            result = MAX( MIN(height[i],height[j])*(j-i) ,result );
        }
    }
    return result;
}
