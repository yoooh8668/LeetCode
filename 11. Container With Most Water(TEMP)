

int maxArea(int* height, int heightSize){
    int result=0;
    int level =0;
    for (int i=0 ; i<heightSize ; i++)
    {
        for (int j=i+1 ; j<heightSize ; j++)
        {
            level = (height[j] < height[i]) ? height[j]:height[i];
            result = (result > level*(j-i)) ? result :  level*(j-i);
        }
    }
    return result;
}
