bool checkInternel (char *s ,int i , int j)
{
    printf("\ni=%d j=%d\n",i,j);
    if (s[i]==')' || s[i]==']' || s[i]=='}') //if not start
    {
        printf("START WITH END CHAR");
        return false;
    }
    int p =j;
    while (p!=i)
    {
        if ( (s[i] == '(' && s[p] ==')') || (s[i] == '[' && s[p] ==']') || (s[i] == '{' && s[p] =='}') )
        {
            if ((p-i) == 1) // p is next to i
            {
                printf("TRUE");
                return true;   
            }
            else
            {
                printf("STILL CHECK FOR %d %d\n",i+1,p-1);
                return checkInternel(s,i+1,p-1);   
            }
        }
        p--;
    }
    return false;
}

bool isValid(char * s){
    int sLen = strlen(s); //1 <= s.length <= 10^4
    printf("sLen=%d\n",sLen);
    if (sLen == 1)
        return false;
    return checkInternel(s, 0, sLen-1);
}
